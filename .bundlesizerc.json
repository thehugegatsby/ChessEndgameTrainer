{
  "files": [
    {
      "path": ".next/static/chunks/app-pages-internals-*.js",
      "maxSize": "100 kB",
      "compression": "gzip"
    },
    {
      "path": ".next/static/chunks/main-*.js",
      "maxSize": "50 kB",
      "compression": "gzip"
    },
    {
      "path": ".next/static/chunks/pages/**/*.js",
      "maxSize": "150 kB",
      "compression": "gzip"
    },
    {
      "path": ".next/static/chunks/*.js",
      "maxSize": "300 kB",
      "compression": "gzip"
    }
  ],
  "ci": {
    "trackBranches": ["main", "develop", "feature/*"],
    "repoBranchBase": "main"
  }
}
