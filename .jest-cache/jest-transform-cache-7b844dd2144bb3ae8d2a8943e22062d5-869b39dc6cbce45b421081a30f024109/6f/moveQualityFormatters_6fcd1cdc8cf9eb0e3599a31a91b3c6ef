    }
};
const getQualityLabel = (quality)=>{
    switch(quality){
        case "excellent":
            return "Excellent";
        case "good":
            return "Good";
        case "inaccuracy":
            return "Inaccuracy";
        case "mistake":
            return "Mistake";
        case "blunder":
            return "Blunder";
        case "unknown":
        default:
            return "Unknown";
    }
};
const formatQualityTooltip = (result)=>{
    const emoji = getQualityEmoji(result.quality);
    const label = getQualityLabel(result.quality);
    return `${emoji} ${label}: ${result.reason}`;
};

