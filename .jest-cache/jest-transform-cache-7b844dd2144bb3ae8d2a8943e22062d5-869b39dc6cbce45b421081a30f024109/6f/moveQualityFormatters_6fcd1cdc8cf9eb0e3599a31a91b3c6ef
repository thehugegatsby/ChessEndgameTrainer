const getQualityEmoji = (quality)=>{
    switch(quality){
        case "excellent":
            return "ðŸ†";
        case "good":
            return "ðŸ‘";
        case "inaccuracy":
            return "âš ï¸";
        case "mistake":
            return "âŒ";
        case "blunder":
            return "ðŸ’¥";
        case "unknown":
        default:
