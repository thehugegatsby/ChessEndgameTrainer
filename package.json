{
  "name": "endgamebook",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "next dev -p 3002",
    "dev:e2e": "cross-env NEXT_PUBLIC_IS_E2E_TEST=true IS_E2E_TEST=true next dev -p 3002",
    "dev:config": "node scripts/dev-server.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:business": "jest --config=tests/jest.config.business.js --coverage",
    "test:coverage:evaluation": "jest --config=config/jest/jest.config.feature-coverage.js --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:performance": "jest --testPathPattern=tests/performance",
    "test:regression": "jest --testPathPattern=tests/regression",
    "test:e2e": "cross-env IS_E2E_TEST=true playwright test",
    "test:e2e:headed": "cross-env IS_E2E_TEST=true playwright test --headed",
    "test:e2e:ui": "cross-env IS_E2E_TEST=true playwright test --ui",
    "test:e2e:critical": "cross-env IS_E2E_TEST=true playwright test tests/e2e/critical",
    "_test:firebase": "cross-env IS_E2E_TEST=true playwright test --config=config/playwright/playwright.firebase.config.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance && npm run test:regression && npm run test:e2e",
    "test:ci": "npm run test:all",
    "kill-port": "node scripts/kill-port.js",
    "validate:agent-config": "node scripts/validate-agent-config.js",
    "mobile:start": "cd app/mobile && expo start",
    "mobile:android": "cd app/mobile && expo start --android",
    "mobile:ios": "cd app/mobile && expo start --ios",
    "mobile:web": "cd app/mobile && expo start --web",
    "check-duplicates": "node scripts/check-duplicate-components.js",
    "check-unused-services": "node scripts/check-unused-services.js",
    "check-unused-files": "node scripts/find-unused-files.js",
    "analyze-unused": "node scripts/deep-unused-analysis.js",
    "analyze-code": "npm run check-duplicates && npm run check-unused-services && npm run check-unused-files",
    "pre-commit": "npm run check-duplicates && npx tsc --noEmit",
    "clean:tests": "node tests/scripts/cleanup.js",
    "tsc": "tsc",
    "test:smoke": "jest --config=tests/jest.config.js --testPathPattern=smoke",
    "audit:fix": "npm audit fix",
    "outdated": "npm outdated",
    "ci:install": "npm ci --prefer-offline --no-audit",
    "ci:test": "npm run lint && npm run tsc -- --noEmit && npm test",
    "migrate:firestore": "ts-node --project tsconfig.scripts.json -r tsconfig-paths/register scripts/migrateToFirestore.ts",
    "migrate:verify": "ts-node --project tsconfig.scripts.json -r tsconfig-paths/register scripts/migrateToFirestore.ts --verify",
    "emulator": "firebase emulators:start",
    "fix:position-ids": "npx tsx scripts/renamePositionIds.ts",
    "seed:test-positions": "npx tsx scripts/seedTestPositions.ts",
    "emulator:test": "firebase emulators:start --only firestore"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "autoprefixer": "^10.4.17",
    "chess.js": "^1.0.0-beta.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "expo-status-bar": "~1.11.1",
    "firebase": "^11.10.0",
    "immer": "^10.1.1",
    "lucide-react": "^0.331.0",
    "nanoid": "^5.1.5",
    "next": "^15.3.3",
    "postcss": "^8.4.35",
    "react": "18.3",
    "react-chessboard": "^2.1.3",
    "react-dom": "18.3",
    "stockfish.wasm": "^0.10.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.53.2",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "axe-playwright": "^2.1.0",
    "babel-jest": "^30.0.4",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^17.0.1",
    "eslint": "^9.31.0",
    "eslint-config-next": "^15.3.5",
    "express": "^5.1.0",
    "firebase-admin": "^13.4.0",
    "glob": "^10.3.10",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "msw": "^2.10.4",
    "next-router-mock": "^1.0.2",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3",
    "wait-on": "^8.0.3",
    "worker-loader": "^3.0.8"
  },
  "description": "Ein modernes Tool zum Training von Schachendspielen mit Spaced Repetition.",
  "main": "jest.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
